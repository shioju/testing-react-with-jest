{
  "name": "intro-to-jest",
  "version": "1.0.0",
  "description": "simple code example for intro to jest",
  "author": "Shioju <shioju12@gmail.com>",
  "license": "MIT",
  "private": true,
  "dependencies": {
    "react": "0.13.x",
    "react-tools": "0.13.x"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-jest": "*",
    "babel-loader": "^5.3.2",
    "css-loader": "^0.17.0",
    "jest-cli": "^0.5.4",
    "style-loader": "^0.12.3",
    "webpack": "^1.12.1"
  },
  "scripts": {
    "test": "node_modules/jest-cli/bin/jest.js"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-tools"
    ],
    "verbose": true,
    "collectCoverage": true
  }
}
